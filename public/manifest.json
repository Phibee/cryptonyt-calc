{
	"short_name": "CryptoNytCalculator",
	"name": "CryptoNytCalculator",
	"description": "A crypto calculator made by Phibee Deinla #malasakit",
	"permissions": ["activeTab"],
	"background": {
		"service_worker": "chrome.js"
	},
	"version": "1.0.0",
	"version_name": "1.0.0",
	"manifest_version": 3,
	"icons": {
		"16": "cryptocalc-logo.png",
		"32": "cryptocalc-logo.png",
		"48": "cryptocalc-logo.png",
		"128": "cryptocalc-logo.png"
	},
    "web_accessible_resources":[
        {
            "matches": ["<all_urls>"],
            "resources": ["index.html"]
        }
      ],
	"action": {
		"default_icon": "cryptocalc-logo.png"
	}
}
